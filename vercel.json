{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py",
            "headers": {
                "cache-control": "s-maxage=1, stale-while-revalidate=59",
                "pragma": "no-cache",
                "expires": "0"
            }
        }
    ],
    "github": {
        "enabled": true,
        "silent": false,
        "autoAlias": true
    },
    "buildCommand": "pip install -r requirements.txt",
    "outputDirectory": ".",
    "public": true
}
