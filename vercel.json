{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "DATABASE_URL": "@medical-management-database-url"
    },
    "buildCommand": "pip install -r requirements.txt",
    "outputDirectory": ".",
    "public": true
}
